@model Department;
@{
    ViewData["Title"] = "Detail";
}

<h1>Detail</h1>

<b>@Model.name</b> <br/> 
Department Code: @Model.id <br />
Full Name: @Model.name <br/>
Location: @Model.location <br/> <br/>

<h2>Employees in @Model.name</h2>

@*Casting the List Employees below because error with in front of 
    of ViewData because ViewData was not allowing. Cant pass 2
    models and ViewData was not recognizing the type.
*@

@foreach(Employee emp in (List<Employee>)ViewData["employees"]){
    <div>@emp.firstname  @emp.lastname</div> 
}